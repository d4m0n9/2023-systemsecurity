def rm(self):
    os.chdir(self.model.filePath(self.model.parent(self.index)))
    fname = self.model.fileName(self.index)
    try:
        if not self.model.isDir(self.index):
            os.unlink(fname)  # Corrected typo here
            print(fname + ' 파일 삭제')
        else:
            shutil.rmtree(fname)
            print(fname + ' 폴더 삭제')
    except Exception as e:
        print("Error:", e)
